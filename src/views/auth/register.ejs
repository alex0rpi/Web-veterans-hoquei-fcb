<%- include('../includes/header.ejs') %>
<body>
  <div class="position-fixed h-100 w-100 bg-primary">
    <%- include('../includes/adminNavigation.ejs') %>
    <div class="container-lg mt-6 bg-light p-4 rounded-4">
      <form action="/auth/login" method="post">
        <div class="form-floating mb-3">
          <input
            type="email"
            class="form-control"
            id="emailInput"
            placeholder="name@example.com"
          />
          <label for="emailInput">Email address</label>
        </div>
        <div class="form-floating mb-3">
          <input
            type="name"
            class="form-control"
            id="nameInput"
            placeholder="name@example.com"
          />
          <label for="nameInput">El teu nom</label>
        </div>
        <div class="form-floating">
          <input
            type="password"
            class="form-control"
            id="passwordInput"
            placeholder="Password"
          />
          <label for="passwordInput">Password</label>
        </div>
        <button type="submit" class="btn mt-4 btn-primary">Registrar</button>
      </form>
      <h4 class="mt-3">Ja tens un compte? Inicia sessi√≥</h4>
    <a href="/auth/login" >
        <button type="button" class="btn btn-primary">Login</button>
    </a>
    </div>
  </div>

  <script src="js/bootstrap.bundle.min.js"></script>
  <script src="js/script.js"></script>
</body>
</html>


